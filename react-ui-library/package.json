{
  "name": "@syedtaqi95/react-ui-library",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --ext js,jsx,ts,tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "prepare": "cd .. && husky react-ui-library/.husky"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-ui-library.es.js",
      "require": "./dist/react-ui-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/react-ui-library.umd.js",
  "module": "./dist/react-ui-library.es.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "prettier": "3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vite-plugin-linter": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syedtaqi95/experiments.git"
  },
  "keywords": [],
  "author": "Syed Taqi Haider",
  "bugs": {
    "url": "https://github.com/syedtaqi95/experiments/issues"
  },
  "homepage": "https://github.com/syedtaqi95/experiments",
  "license": "MIT"
}
